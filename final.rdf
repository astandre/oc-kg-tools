  <rdf:Description rdf:about="http://127.0.0.1/ockb/resources/ETHIC2">
    <rdf:type rdf:resource="http://127.0.0.1/ockb/course/ontology/Course"/>
    <ns1:duration>8 semanas</ns1:duration>
    <ns1:courseCode>ETHIC2</ns1:courseCode>
